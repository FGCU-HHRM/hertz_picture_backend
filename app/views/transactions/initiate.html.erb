  <%= f.file_field :north_west %>
  <%= f.label :south %>
  <%= f.file_field :south %>
  <%= f.label :south_east %>
  <%= f.file_field :south_east %>
  <%= f.label :south_west %>
  <%= f.file_field :south_west %>
  <%= f.label :east %>
  <%= f.file_field :east %>
  <%= f.label :west %>
  <%= f.file_field :west %>
  <%= f.label :image1 %>
  <%= f.file_field :image1 %>
  <%= f.label :image2 %>
  <%= f.file_field :image2 %>
  <%= f.label :image3 %>
  <%= f.file_field :image3 %>
  <%= f.label :image4 %>
